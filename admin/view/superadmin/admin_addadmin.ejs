<% include ./layout/dashboard_header.ejs %>
<div id="page-wrapper">
	<div class="row">
		<div class="col-lg-12">
			<h1 class="page-header"><%= sidebar_group_add %></h1>
		</div>
		<!-- /.col-lg-12 -->
	</div>
	<!-- /.row -->
	<div class="row">
		<div class="col-lg-12">
			<div class="panel panel-default">
				<div class="panel-heading">
					你渴望力量么?
				</div>
				<div class="panel-body">
					<form role="form" method="post">
						<div class="form-group">
							<label>Username</label>
							<input class="form-control" name="username">
							<p class="help-block">Input a username here</p>
						</div>
						<div class="form-group">
							<label>Password</label>
							<input type="password" class="form-control" name="password">
							<p class="help-block">Input a password here</p>
						</div>
						<div class="form-group">
							<label>Email</label>
							<input type="email" class="form-control" name="email">
							<p class="help-block">Input email here</p>
						</div>
						<div class="form-group">
							<label>Select a authority</label>
							<div class="radio">
								<label>
									<input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>AddClass Level1
								</label>
							</div>
							<div class="radio">
								<label>
									<input type="radio" name="optionsRadios" id="optionsRadios2" value="option2">AddClass Level2
								</label>
							</div>
							<div class="radio">
								<label>
									<input type="radio" name="optionsRadios" id="optionsRadios3" value="option3">AddClass Level0
								</label>
							</div>
						</div>
						<button type="submit" class="btn btn-primary">Confirm</button>
					</form>
				</div>
			</div>
		</div>
	</div>
</div>
<% include ./layout/dashboard_footer.ejs %>