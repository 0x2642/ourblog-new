<% include subadmin_header.ejs %>
<!-- Page Content -->
<div id="page-wrapper">
	<div class="container-fluid">
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header">你是高贵的狗管理</h1>
			</div>
			<!-- /.col-lg-12 -->
		</div>
	</div>
	<form method="post" action="/admin/subadmin_post">
		<div class="form-group">
			<label>标题录入</label>
			<input name="article_title" class="form-control">
		</div>
		<div class="form-group">
			<label>描述录入</label>
			<input name="article_description" class="form-control">
		</div>
		<div class="form-group">
			<label>文章录入</label>
			<textarea name="article_context" class="form-control" rows="20"></textarea>
		</div>
		<label>tag输入</label>
		<div class="form-group">
			<input name="tag1" class="form-control">
		</div>
		<div class="form-group">
			<input name="tag2" class="form-control">
		</div>
		<div class="form-group">
			<input name="tag3" class="form-control">
		</div>
		<button type="submit" class="btn btn-primary">发布</button>
	</form>
	<!-- /.container-fluid -->
</div>
<!-- /#page-wrapper -->
