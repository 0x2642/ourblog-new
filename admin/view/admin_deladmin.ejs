<% include ./layout/dashboard_header.ejs %>
<div id="page-wrapper">
	<div class="row">
		<div class="col-lg-12">
			<h1 class="page-header">加班协会管理员</h1>
		</div>
		<!-- /.col-lg-12 -->
	</div>
	<!-- /.row -->
	<div class="row">
		<div class="col-lg-12">
			<div class="panel panel-default">
				<div class="panel-heading">
					协会成员信息
				</div>
				<!-- /.panel-heading -->
				<div class="panel-body">
					<div class="dataTable_wrapper">
						<table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
							<thead>
								<tr>
									<th>AddClasser</th>
									<th>Email</th>
									<th>Add Level</th>
									<th>Operation</th>
								</tr>
							</thead>
							<tbody>
								<% admins.forEach(function(admin, idx) { %>
									<%  if (idx % 2 == 0) { %>
										<tr class="even gradeX">
									<% } else { %>
										<tr class="odd gradeC">
									<% } %>
										<td> <%= admin.username %> </td>
										<td> <%= admin.email %> </td>
										<td> <%= admin.level %> </td>
										<td> 
											<form method="post" action="/admin/admin_deladmin/<%= admin.email %>">
												<button type="submit" class="btn btn-primary">Delete</button>
											</form>
										</td>
									</tr>
								<% }) %>
							</tbody>
						</table>
					</div>
				</div>
				<!-- /.panel-body -->
			</div>
			<!-- /.panel -->
		</div>
		<!-- /.col-lg-12 -->
	</div>
	<form method="post">
		<button id="del_all" type="submit" class="btn btn-primary">Delete All</button>
	</form>
<% include ./layout/dashboard_footer.ejs %>